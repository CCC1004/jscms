				type="tns:queryCondition_type" maxOccurs="1" minOccurs="0">
			</element>
			<element name="queryScript" type="tns:queryScript_type"
				maxOccurs="1" minOccurs="0">
				<annotation>
					<documentation>
						<![CDATA[
		        			基于beanShell脚本固定SQL查询条件，配置完后该条件将做为当前App取数据时必需的条件。
		        		]]>
					</documentation>
				</annotation>
			</element>
			<element name="advancedSelect" type="tns:advancedSelect"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="xaxis" type="tns:xaxisType" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="dataSet" type="tns:dataSetType" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="actionButton" type="tns:actionButton"
				maxOccurs="unbounded" minOccurs="0">
			</element>

		</sequence>
		<attribute name="id" type="string" use="required"></attribute>
		<attribute name="caption" type="string" use="required"></attribute>
		<attribute name="XAxislabel" type="string"></attribute>
		<attribute name="YAxislabel" type="string"></attribute>
		<attribute name="width" type="string"></attribute>
		<attribute name="height" type="string"></attribute>
		<attribute name="showValue" type="boolean"></attribute>
		<attribute name="XLabelFormat" type="string" use="optional"></attribute>
		<attribute name="extAttr" type="string" use="optional"></attribute>
		<attribute name="XLabelMax" type="int"  use="optional">
			<annotation>
				<documentation>
					用于指定X轴标注最多显示多少个。为空或0时，表示显示所有标注。当设置了最大个数后，会自动根据总个数设置标注显示时是否跳跃。
				</documentation>
			</annotation>			
		</attribute>
	</complexType>

	<complexType name="xaxisType">
		<sequence>
			<element name="xfield" type="tns:xfieldType" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>

	<complexType name="xfieldType">
		<sequence>
			<element name="key" type="tns:key" maxOccurs="1" minOccurs="0"></element>
		</sequence>
		<attribute name="id" type="string" use="required"></attribute>
		<attribute name="msg" type="string" use="required"></attribute>
	</complexType>

	<complexType name="dataSetType">
		<sequence>
			<element name="dataField" type="tns:dataFieldType" maxOccurs="unbounded" minOccurs="1"></element>
		</sequence>
	</complexType>

	<complexType name="dataFieldType">
		<attribute name="id" type="string" use="required"></attribute>
		<attribute name="msg" type="string" use="required"></attribute>
	</complexType>

	<complexType name="foots_type">
        <annotation>
        	<documentation>列表脚统计行组</documentation>
        </annotation>
        <sequence>
			<element name="foot" type="tns:foot_type" maxOccurs="unbounded" minOccurs="1">
				<annotation>
					<documentation>列表脚统计行</documentation>
				</annotation></element>
		</sequence>
	</complexType>

	<complexType name="foot_type">
        <annotation>
        	<documentation>列表脚统计行</documentation>
        </annotation>
        <attribute name="type" use="required">
            <annotation>
            	<documentation>
            		sum:累计值 avg:平均值 max:最大值 min:最小值
            	</documentation>
            </annotation>
            <simpleType>
				<restriction base="string">
					<enumeration value="sum" />
					<enumeration value="avg" />
					<enumeration value="max" />
					<enumeration value="min" />
				</restriction>
			</simpleType>		
		</attribute>
	</complexType>

	<complexType name="queryCondition_type">
        <annotation>
        	<documentation><![CDATA[
        		固定SQL查询条件，配置完后该条件将做为当前App取数据时必需的条件。
        	]]></documentation>
        </annotation>
        <sequence>
			<element name="param" type="tns:paramType" maxOccurs="unbounded" minOccurs="0">
				<annotation>
					<documentation>动态参数</documentation>
				</annotation></element>
		</sequence>
		<attribute name="where" type="string" use="required">
			<annotation>
				<documentation>
					<![CDATA[				
					SQL查询条件。 可支持动态参数，如addv=${addv}， 动态参数以${ }为标识符，
					该动态参数必需结合param子结点使用， param里id的名称和${name}里的name对应 如:
					<BR>
					<blockquote><pre>
					&lt;queryCondition where=&quot;addv=${addvcd}&quot;&gt;
						&lt;param id=&quot;addvcd&quot; value=&quot;addvId&quot;
							scope=&quot;request&quot;&gt;
					&lt;/queryCondition&gt;
					</pre></blockquote>
				]]>
				</documentation>
			</annotation></attribute>
	</complexType>
	
	<complexType name="queryScript_type">
        <annotation>
        	<documentation><![CDATA[
        		beanShell固定SQL查询条件，配置完后该条件将做为当前App取数据时必需的条件。
        	]]></documentation>
        </annotation>
        <sequence>
        	<element name="imports" type="tns:importType" maxOccurs="unbounded" minOccurs="0">
				<annotation>
					<documentation>
						<![CDATA[			
						需要导入的包或者类名<br>
						书写格式:直接写包名+类名，以;结尾，比如qlight.frames.security.SecurityAuthContext;
						]]>
					</documentation>
				</annotation>
			</element>
			<element name="param" type="tns:paramType" maxOccurs="unbounded" minOccurs="0">
				<annotation>
					<documentation>动态参数</documentation>
				</annotation></element>
				<element name="script" type="string" maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation>
						beanShell脚本  返回变量必须是where
						会将where的值当成查询条件做过滤
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="importType">
		<attribute name="value" type="string" use="required">
			<annotation>
				<documentation>
					<![CDATA[			
					需要导入的包或者类名<br>
					书写格式:直接写包名+类名，以;结尾，比如qlight.frames.security.SecurityAuthContext;
					]]>
				</documentation>
			</annotation>
		</attribute>
	</complexType>
	

	<element name="project">
		<complexType>
			<sequence>
				<element name="multiDBSource" maxOccurs="1"
					minOccurs="1">
					<complexType>
						<sequence>
							<element name="DBSource"
								maxOccurs="unbounded" minOccurs="1">
								<complexType>

									<attribute name="id" type="string"
										use="required">
									</attribute>
									<attribute name="managerBean"
										type="string" use="required">
									</attribute>
									<attribute name="dataSourceBean"
										type="string" use="required">
									</attribute>
									<attribute name="baseDaoBean"
										type="string" use="required">
									</attribute>
								</complexType>
							</element>
						</sequence>
						<attribute name="mainSource" type="string"></attribute>
					</complexType>
				</element>



				<element name="models" type="tns:models_type" maxOccurs="1" minOccurs="1"></element>
			</sequence>
		</complexType>
	</element>



	<complexType name="models_type">
		<sequence>
			<element name="model" type="tns:Model_type" maxOccurs="unbounded" minOccurs="0"></element>
		</sequence>
	</complexType>

	<complexType name="Model_type">
		<sequence>
			<element name="viewXml" type="tns:viewXml_type" maxOccurs="1" minOccurs="0">
				<annotation>
					<documentation>设置viewXml配置文件的标签</documentation>
				</annotation></element>
			<element name="service" type="tns:serviceType" maxOccurs="1" minOccurs="0">
				<annotation>
					<documentation>
						设置模型自定义service的标签，可通过该标签指定该模型的自定义service，service必需现实GenericService接口，或者继承自GenericServiceImpl类并覆盖要修改的方法。
					</documentation>
				</annotation></element>
		</sequence>
		<attribute name="id" type="string" use="required">
			<annotation>
				<documentation>
					模型标识，一般同对应的表名称一致，如果是非主数据源的表，需要在表名前加上数据源名称作为前缀，如：wr.addv
				</documentation>
			</annotation></attribute>
		<attribute name="pojoClass" type="string" use="required">
			<annotation>
				<documentation>对应的POJO Class</documentation>
			</annotation></attribute>
	</complexType>

	<complexType name="viewXml_type">
		<attribute name="path" type="string" use="required">
			<annotation>
				<documentation>viewXml的保存路径</documentation>
			</annotation></attribute>
	</complexType>

	<complexType name="serviceType">
		<attribute name="beanId" type="string" use="required">
			<annotation>
				<documentation>自定义Service的SpringBean Id</documentation>
			</annotation></attribute>
	</complexType>

	<complexType name="trendLine_Type">
		<attribute name="lineName" type="string" use="required">
			<annotation>
				<documentation>线的名称</documentation>
			</annotation>
		</attribute>
		<attribute name="startValue" type="string" use="required">
			<annotation>
				<documentation>
					<![CDATA[
					    该属性用于设置线的开始值，可直接设置常量值或者也可分别可从Application，Session，Request,Service里取值。<br>
					    <br>常量值时，直接写数值即可；如果从Application、Request等里取值时，需要以#号打头后面加上值来源及参数名称，格式为：#[值来源].[参数名]，示例写法如：<br>
					    #request.value,#service.getLineValue。<br>
					       各取值来源的说明如下：
						<dl>
						<dt>Application,Session,Request类型:</dt>
						<dd>用于从Application,Session,Request的上下文里取值</dd>
						<dt>service类型：</dt>
						<dd>用于通过指定的Bean获取值，将参数设置为SpringBean的id，定义的bean必须实现ValueService接口。</dd>							 						
						</dl>
					]]>
				</documentation>
			</annotation>
		</attribute>
		<attribute name="endValue" type="string" use="optional">
			<annotation>
				<documentation>
					<![CDATA[
					    该属性用于设置线的结束值（可以不设该结束值）。可直接设置常量值或者也可分别可从Application，Session，Request,Service里取值。<br>
					    <br>常量值时，直接写数值即可；如果从Application、Request等里取值时，需要以#号打头后面加上值来源及参数名称，格式为：#[值来源].[参数名]，示例写法如：<br>
					    #request.value,#service.getLineValue。<br>
					       各取值来源的说明如下：
						<dl>
						<dt>Application,Session,Request类型:</dt>
						<dd>用于从Application,Session,Request的上下文里取值</dd>
						<dt>service类型：</dt>
						<dd>用于通过指定的Bean获取值，将参数设置为SpringBean的id，定义的bean必须实现ValueService接口。</dd>							 						
						</dl>
					]]>
				</documentation>
			</annotation>
		</attribute>
		<attribute name="color" type="string" use="required">
			<annotation>
				<documentation>线的颜色。用16进制表示，如：FF00BB</documentation>
			</annotation>
		</attribute>
		<attribute name="showOnTop" type="boolean" use="optional">
			<annotation>
				<documentation>线是否显示在最上层。默认为false。</documentation>
			</annotation></attribute>
	</complexType>
</schema>